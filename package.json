{
  "private": true,
  "packageManager": "pnpm@8.6.7",
  "author": "Alan Lu <alanlu.zyl@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=18.16.0",
    "npm": "use pnpm please!",
    "yarn": "use pnpm please!",
    "pnpm": ">=8.6.0"
  },
  "scripts": {
    "build": "turbo run build",
    "build:clean": "rm -rf ./packages/**/*dist && turbo run build --force --no-cache",
    "check": "npm run commit-check && turbo run test lint type-check --parallel",
    "commit-check": "commitlint --from=HEAD~1 --verbose",
    "test": "turbo run test --parallel",
    "lint": "turbo run lint --parallel",
    "type-check": "turbo run type-check",
    "dev": "turbo run dev --concurrency=20",
    "preinstall": "npx -y only-allow pnpm",
    "postinstall": "npx simple-git-hooks",
    "commit": "pnpm czg",
    "update": "pnpm update -r"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@element-plus/nuxt": "^1.0.5",
    "@formkit/addons": "0.18.0-23c4d8f",
    "@formkit/icons": "0.18.0-23c4d8f",
    "@formkit/nuxt": "0.18.0-23c4d8f",
    "@formkit/zod": "0.18.0-23c4d8f",
    "@intlify/unplugin-vue-i18n": "^0.12.2",
    "@master/css-extractor": "^2.0.0-beta.166",
    "@master/css-extractor.vite": "^2.0.0-beta.166",
    "@master/css-server.nitro": "^2.0.0-beta.166",
    "@master/css-validator": "^2.0.0-beta.166",
    "@master/css.vue": "^2.0.0-beta.166",
    "@nuxt/content": "^2.7.2",
    "@nuxt/devtools": "^0.6.7",
    "@nuxt/image": "1.0.0-rc.1",
    "@nuxtjs/color-mode": "^3.3.0",
    "@nuxtjs/fontaine": "^0.4.1",
    "@nuxtjs/google-fonts": "^3.0.2",
    "@nuxtjs/i18n": "8.0.0-beta.13",
    "@pinia-plugin-persistedstate/nuxt": "^1.1.1",
    "@pinia/nuxt": "^0.4.11",
    "@types/node": "^18.17.1",
    "@vueuse/core": "^10.3.0",
    "@vueuse/nuxt": "^10.3.0",
    "@zy-kit/components": "workspace:*",
    "@zy-kit/config": "workspace:*",
    "@zy-kit/master": "workspace:*",
    "@zy-kit/scss": "workspace:*",
    "@zy-kit/utils": "workspace:*",
    "class-variant": "^2.0.0-beta.166",
    "cz-git": "^1.7.0",
    "czg": "^1.7.0",
    "element-plus": "^2.3.8",
    "eslint": "^8.46.0",
    "eslint-config-custom": "workspace:*",
    "fast-glob": "^3.3.1",
    "lint-staged": "^13.2.3",
    "markdownlint-cli2": "^0.7.1",
    "markdownlint-config-custom": "workspace:*",
    "nuxt": "^3.6.5",
    "nuxt-icon": "^0.4.2",
    "pinia": "^2.1.6",
    "postcss": "^8.4.27",
    "postcss-preset-env": "^9.1.0",
    "prettier": "^2.8.8",
    "prettier-config-custom": "workspace:*",
    "sass": "^1.64.1",
    "simple-git-hooks": "^2.9.0",
    "stylelint": "^15.10.2",
    "stylelint-config-custom": "workspace:*",
    "tsup": "^7.1.0",
    "turbo": "^1.10.12",
    "typescript": "5.1.6",
    "vue-tsc": "^1.8.8"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "commit-msg": "pnpm commitlint --edit $1"
  }
}
